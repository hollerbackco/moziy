
#video-manage
  #viewer
  #main-content
    = render :partial => 'manage/channels/channel_menu'
    = form_for ['manage', @channel, @video] do |f|
      .field
        %strong Add Videos
        = label_tag :links, 'Enter a comma separated list of links'
        %br
        = text_area_tag :links
      .actions
        = f.submit "Add to Channel"
