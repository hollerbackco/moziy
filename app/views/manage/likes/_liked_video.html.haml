- if a.channel.present?
  %li.video-item.liked-video.clearfix{:id => "video_#{a.video_id.to_s}", class: a.state}
    = image_tag a.video.video_image_url(:thumb_list), class: "image"
    .title= link_to a.video.title, manage_like_path(a)
    .meta
      = link_to a.channel.title, slug_path(a.channel.slug),
        class: "channel-link channel"
      %span.notes= pluralize a.history.count, "attachments"
      = link_to start_video_url(a.channel.slug, v: a.id), start_video_path(a.channel.slug, v: a.id),
        class: "permalink"
